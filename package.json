{
	"name": "minecraft-server-util",
	"version": "3.4.4",
	"description": "A Node.js library for Minecraft servers that can retrieve status, perform queries, and RCON into servers.",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"scripts": {
		"prepublishOnly": "npm run clean && npm run build && npm run test && npm run lint",
		"build": "tsc",
		"clean": "rimraf build/",
		"test": "mocha --require ts-node/register test/**/*.ts --timeout 30000",
		"coverage": "nyc npm test",
		"lint": "eslint ."
	},
	"keywords": [
		"minecraft",
		"java",
		"bedrock",
		"server",
		"status",
		"ping",
		"query",
		"rcon"
	],
	"author": "Jacob Gunther",
	"license": "MIT",
	"devDependencies": {
		"@types/chai": "^4.2.14",
		"@types/mocha": "^8.2.0",
		"@types/node": "^14.14.13",
		"@typescript-eslint/eslint-plugin": "^4.9.1",
		"@typescript-eslint/parser": "^4.9.1",
		"chai": "^4.2.0",
		"eslint": "^7.15.0",
		"mocha": "^8.2.1",
		"nyc": "^15.1.0",
		"rimraf": "^3.0.2",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"bugs": {
		"url": "https://github.com/PassTheMayo/minecraft-server-util"
	},
	"repository": {
		"url": "https://github.com/PassTheMayo/minecraft-server-util",
		"type": "git"
	},
	"dependencies": {
		"ansi-styles": "^6.0.0"
	}
}